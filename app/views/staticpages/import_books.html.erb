saved: <%= @book_save %><br>
failed: <%= @book_not_save %><br>

<table class="table">
	<% @lines.each do |line| %>
		<%row = line.chop.split("\t")%>
	<tr>
			<td><%=row[0]%></td>
			<td><%=row[1]%></td>
			<td><%=row[2]%></td>
			<td><%=row[3]%></td>
			<td><%=row[4]%></td>
			<td><%=row[5]%></td>
			<td><%=row[6]%></td>
			<td><%=row[7]%></td>
			<td><%=row[8]%></td>
			<td><%=row[9]%></td>
	</tr>
	<% end%>

</table>

<%= link_to "Home", root_path, :class => "btn btn-default"%>